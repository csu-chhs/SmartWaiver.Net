<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>CHHS Application Developers</Authors>
    <Company>College of Health and Human Sciences</Company>
    <PackageId>SmartWaiver.Net</PackageId>
    <PackageDescription>Smart Waiver Client Library</PackageDescription>
    <RepositoryUrl>https://github.com/csu-chhs/SmartWaiver.Net</RepositoryUrl>
    <Version>1.0.3</Version>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RestSharp" Version="106.15.0" />
    <PackageReference Include="RestSharp.Serializers.NewtonsoftJson" Version="106.12.0"/>
  </ItemGroup>

</Project>